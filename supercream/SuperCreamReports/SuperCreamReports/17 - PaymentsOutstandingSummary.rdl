        </Field>
        <Field Name="NoOfUnits">
          <DataField>NoOfUnits</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InvoiceDate">
          <DataField>InvoiceDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PercentageValue">
          <DataField>PercentageValue</DataField>
          <rd:TypeName>System.Single</rd:TypeName>
        </Field>
        <Field Name="InvoiceNo">
          <DataField>InvoiceNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VatExemptible">
          <DataField>VatExemptible</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="LineCredit">
          <DataField>LineCredit</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LinePrice">
          <DataField>LinePrice</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="OrderArbitraryCreditTotal">
          <DataField>OrderArbitraryCreditTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select oh.ID, AlphaID, InvoiceNo, c.Name, InvoiceDate, p.Description, ol.NoOfUnits, v.PercentageValue, 
	v.VatExemptible,	
	case when p.VatExempt = 1 then round(isnull(ocnl.Price, 0),2) 
		else round(isnull(ocnl.Price, 0) * (1 + (v.PercentageValue / 100.0)), 2) end LineCredit,                     				
	case when p.VatExempt = 1 THEN round((ol.Price * ol.NoOfUnits), 2) 
		else round(ol.Price * ol.NoOfUnits * (1 + (v.PercentageValue / 100.0)),2) end
			LinePrice,
		dbo.GetArbiraryCreditNoteTotal(oh.ID) OrderArbitraryCreditTotal
	from orderheader oh	
inner join OrderNotesStatus os
on oh.ID = os.OrderID
inner join Customer c on
c.ID = oh.CustomerId
inner join OutletStore ots ON
	ots.CustomerID = c.ID
inner join OrderLine ol
	on oh.ID = ol.OrderID
inner join Product p on
	ol.ProductID = p.ID
inner join VatCode v on
	oh.VatCodeID = v.ID
left outer join OrderCreditNoteLine AS ocnl 
	ON ol.ID = ocnl.OrderLineID
where (OrderStatus = dbo.ConstInvoice() or OrderStatus = dbo.ConstInvoicePrinted())
and InvoicePaymentComplete = dbo.ConstPaymentNotComplete()
and oh.InvoiceDate &gt;= @InvoiceStartDate AND oh.InvoiceDate &lt;= @InvoiceEndDate
order by oh.InvoiceDate desc, c.name, oh.alphaid desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@InvoiceStartDate">
            <Value>=Parameters!InvoiceStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceEndDate">
            <Value>=Parameters!InvoiceEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
