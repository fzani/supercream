        <CommandType>Text</CommandType>
        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
SELECT        SpecialInvoiceHeader.InvoiceNo, SpecialInvoiceCreditNote.CreditAmount, SpecialInvoiceCreditNote.Reason, SpecialInvoiceCreditNote.DateCreated, 
                         SpecialInvoiceCreditNote.Reference, SpecialInvoiceCreditNote.VatExempt, SpecialInvoiceCreditNote.ReasonForVoiding
FROM            SpecialInvoiceCreditNote INNER JOIN
                         SpecialInvoiceHeader ON SpecialInvoiceCreditNote.SpecialInvoiceID = SpecialInvoiceHeader.ID
order by SpecialInvoiceCreditNote.DueDate desc</CommandText>
        <Timeout>0</Timeout>
      </Query>
