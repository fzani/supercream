      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with Vat(OrderDeliveryDate, VatAmount)
as
(
	select DeliveryDate, Round(Sum(NoOfUnits * UnitPrice) * (v.PercentageValue / 100),2) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID
	where p.VatExempt = 0
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	group by oh.DeliveryDate, v.PercentageValue
),
VatableAmount(OrderDeliveryDate, VatableAmount)
as
(
	select DeliveryDate, Round(Sum(NoOfUnits * UnitPrice),2) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID
	where p.VatExempt = 0
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	group by oh.DeliveryDate, v.PercentageValue
),
NonVatableAmount(OrderDeliveryDate, NonVatableAmount)
as
(
	select DeliveryDate, Round(Sum(NoOfUnits * UnitPrice),2) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID
	where p.VatExempt = 1
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	group by oh.DeliveryDate, v.PercentageValue
),
CreditVatAmount(OrderDeliveryDate, VatAmount)
as
(
	select DeliveryDate, isnull(Round(Sum(ocnl.NoOfUnits * ocnl.Price) * (v.PercentageValue / 100),2), 0) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID	
	left join OrderCreditNoteLine ocnl on
		ol.ID = ocnl.OrderLineID		
	where p.VatExempt = 0
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	group by oh.DeliveryDate, v.PercentageValue
),
CreditVatableAmount(OrderDeliveryDate, VatableAmount)
as
(
	select DeliveryDate, Round(Sum(ocnl.NoOfUnits * ocnl.Price),2) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID	
	left join OrderCreditNoteLine ocnl on
		ol.ID = ocnl.OrderLineID		
	where p.VatExempt = 0
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	group by oh.DeliveryDate, v.PercentageValue
),
CreditNonVatableAmount(OrderDeliveryDate, NonVatableAmount)
as
(
	select  DeliveryDate, Round(Sum(ocnl.NoOfUnits * ocnl.Price),2) from orderheader oh
	inner join orderline ol on
		oh.ID = ol.orderid
	inner join Product p on
		ol.ProductID = p.ID
	inner join VatCode v on 
		oh.VatCodeID = v.ID	
	left join OrderCreditNoteLine ocnl on
		ol.ID = ocnl.OrderLineID		
	where p.VatExempt = 1
	and (oh.DeliveryDate &gt;= @StartDate and oh.DeliveryDate &lt;= @EndDate)
	and (oh.OrderStatus = dbo.ConstInvoice() or oh.OrderStatus = dbo.ConstInvoicePrinted())
	group by oh.DeliveryDate, v.PercentageValue
),
ArbitraryCreditNoteVat(DateCreated, VatAmount) as
(
	select DateCreated, Round(sum(CreditAmount) * (v.PercentageValue / 100),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 0
		and (cr.DateCreated &gt;= @StartDate and cr.DateCreated &lt;= @EndDate)
		group by DateCreated, v.PercentageValue
),
ArbitraryCreditNoteVatableAmount(DateCreated, VatableAmount) as
(
	select DateCreated, Round(sum(CreditAmount),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 0
		and (cr.DateCreated &gt;= @StartDate and cr.DateCreated &lt;= @EndDate)
		group by DateCreated, v.PercentageValue
),
ArbitraryCreditNoteNonVatableAmount(DateCreated, NonVatableAmount) as
(
	select DateCreated, Round(sum(CreditAmount),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 1
		and (cr.DateCreated &gt;= @StartDate and cr.DateCreated &lt;= @EndDate)
		group by DateCreated, v.PercentageValue, VatExemptible
)
select vat.OrderDeliveryDate, isnull(vat.VatAmount, 0) Vat, 
		isnull(nvv.NonVatableAmount, 0) as NonVatableAmount, 
		isnull(va.VatableAmount, 0) VatableAmount,
		Round(isnull(vat.VatAmount,0) + isnull(nvv.NonVatableAmount, 0) + isnull(va.VatableAmount, 0),2) VatTotal,
		isnull(cva.VatAmount,0) + isnull(acnv.VatAmount, 0) CreditVat,
		isnull(cvta.VatableAmount, 0) + isnull(acnva.VatableAmount, 0) CreditVatableAmount,
		isnull(cvnta.NonVatableAmount, 0) + isnull(acnnva.NonVatableAmount, 0) CreditNonVatableAmount,
		Round(isnull(cva.VatAmount,0) + isnull(cvnta.NonVatableAmount, 0) + isnull(cvta.VatableAmount, 0),2) CreditTotal
	from Vat vat
	left join NonVatableAmount nvv on
		vat.OrderDeliveryDate = nvv.OrderDeliveryDate
	left join VatableAmount va on
		vat.OrderDeliveryDate = va.OrderDeliveryDate	
	left join CreditVatAmount cva on
		vat.OrderDeliveryDate = cva.OrderDeliveryDate
	left join CreditVatableAmount cvta on
		vat.OrderDeliveryDate = cvta.OrderDeliveryDate
	left join CreditNonVatableAmount cvnta on
		vat.OrderDeliveryDate = cvnta.OrderDeliveryDate
	left join ArbitraryCreditNoteVat acnv on
		vat.OrderDeliveryDate = acnv.DateCreated
	left join ArbitraryCreditNoteVatableAmount acnva on
		vat.OrderDeliveryDate = acnva.DateCreated
	left join ArbitraryCreditNoteNonVatableAmount acnnva on
		vat.OrderDeliveryDate = acnnva.DateCreated
		
	
order by vat.OrderDeliveryDate</CommandText>
