  <DataSets>
    <DataSet Name="CreditNoteDS">
      <Fields>
        <Field Name="DueDate">
          <DataField>DueDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreditVatAmount">
          <DataField>CreditVatAmount</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CreditVatableAmount">
          <DataField>CreditVatableAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NonVatableAmount">
          <DataField>NonVatableAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="VatTotal">
          <DataField>VatTotal</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SCDataSource</DataSourceName>
        <CommandText>with ArbitraryCreditNoteVat(DueDate, CreditVatAmount) as
(
	select DueDate, Round(sum(CreditAmount) * (v.PercentageValue / 100),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 0
		and (cr.DueDate &gt;= @StartDueDate and cr.DueDate &lt;= @EndDueDate)
		group by DueDate, v.PercentageValue
),
ArbitraryCreditNoteVatableAmount(DueDate, VatableAmount) as
(
	select DueDate, Round(sum(CreditAmount),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 0
		and (cr.DueDate &gt;= @StartDueDate and cr.DueDate &lt;= @EndDueDate)
		group by DueDate, v.PercentageValue
),
ArbitraryCreditNoteNonVatableAmount(DueDate, NonVatableAmount) as
(
	select DueDate, Round(sum(CreditAmount),2) from CreditNote cr
		inner join OrderHeader oh on
			cr.OrderID = oh.ID
		inner join VatCode v on
			oh.VatCodeID = v.ID
		where cr.VatExempt = 1
		and (cr.DueDate &gt;= @StartDueDate and cr.DueDate &lt;= @EndDueDate)
		group by DueDate, v.PercentageValue, VatExemptible
)
select arVat.DueDate, isnull(arVat.CreditVatAmount, 0) CreditVatAmount,
		isnull(nva.VatableAmount, 0) as CreditVatableAmount, 
		isnull(nnva.NonVatableAmount, 0) as NonVatableAmount,
		Round(isnull(arVat.CreditVatAmount,0) + isnull(nva.VatableAmount, 0) + isnull(nnva.NonVatableAmount, 0),2) VatTotal
	from ArbitraryCreditNoteVat arVat
	left join ArbitraryCreditNoteVatableAmount nva on
		arVat.DueDate = nva.DueDate
	left join ArbitraryCreditNoteNonVatableAmount nnva on
		arVat.DueDate = nnva.DueDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDueDate">
            <Value>=Parameters!StartDueDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDueDate">
            <Value>=Parameters!EndDueDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
