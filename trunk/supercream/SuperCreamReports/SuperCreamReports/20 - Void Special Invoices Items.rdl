      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
SELECT c.Name
	   ,ac.AlphaID as AccountReference  
	   ,ac.CompanyToInvoiceTo    
		,os.Name as OutletStoreName
		,v.PercentageValue            
      ,[AlphaPrefixOrPostFix]    
      ,[OrderDate]
      ,[DeliveryDate]
      ,[OrderStatus]
      ,sih.[SpecialInstructions]
      ,[InvoiceNo]
      ,[DatePrinted]
      ,[DateReprinted]
      ,[DateCreated]
      ,[DateModified]
      ,[ReasonForVoiding]      
      ,sil.Description
      ,sil.Discount
      ,sil.NoOfUnits
      ,sil.Price
      ,sil.QtyPerUnit
      ,sil.VatExempt
  FROM [SuperCreamDB].[dbo].[SpecialInvoiceHeader] sih
  inner join customer c on
	sih.CustomerID = c.ID
 inner join Account ac on
	sih.AccountID = ac.ID
 inner join outletstore os on
	sih.OutletStoreID = os.ID
 inner join VatCode v on
	sih.VatCodeID = v.ID
 left join SpecialInvoiceLine sil on
	sih.ID = sil.SpecialInvoiceID
	where sih.DeliveryDate between @SpecialInvoiceCreatedDate and @SpecialInvoiceEndDate
	and sih.OrderStatus = 8
	order by c.name, sih.invoiceno, sih.DeliveryDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@SpecialInvoiceCreatedDate">
            <Value>=Parameters!SpecialInvoiceCreatedDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SpecialInvoiceEndDate">
            <Value>=Parameters!SpecialInvoiceEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
