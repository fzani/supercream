          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SuperCreamDB</DataSourceName>
        <CommandText>SELECT        ID, Name
FROM            Customer
ORDER BY Name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="ProductList">
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SuperCreamDB</DataSourceName>
        <CommandText>declare @RecordCount int,
				@ParamResult varchar(50)
		
		declare @CustomerIdList table (id int)
 
    if @p_CustomerIdList is not null

    begin
        set @RecordCount = 1
        set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
 
        while @ParamResult IS NOT NULL
        begin
           insert into @CustomerIdList (id) VALUES (@ParamResult)

            set @RecordCount = @RecordCount + 1
            set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
        end
    end     

select distinct(p.ID), p.Description from OrderHeader oh
	inner join OrderLine ol
		on oh.ID = ol.OrderID
	inner join Product p on
		ol.ProductID = p.ID
	inner join Customer c on
		oh.CustomerID = c.ID
	where oh.OrderStatus in (2,3,6,7)
		and oh.DeliveryDate between @p_DateFrom and @p_DateTo	
		and c.ID in (select id from @CustomerIdList)
order by p.Description</CommandText>
        <QueryParameters>
          <QueryParameter Name="@p_CustomerIdList">
            <Value>=Join(Parameters!p_CustomerIdList.Value,":")</Value>
          </QueryParameter>
          <QueryParameter Name="@p_DateFrom">
            <Value>=Parameters!p_DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_DateTo">
            <Value>=Parameters!p_DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ProductSalesByCustomer">
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductCode">
          <DataField>ProductCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SuperCreamDB</DataSourceName>
        <CommandText>declare @RecordCount int,
				@ParamResult varchar(50)
		
		declare @VatRate decimal(10,2)			
		
		declare @CustomerIdList table (id int)			
 
    if @p_CustomerIdList is not null

    begin
        set @RecordCount = 1
        set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
 
        while @ParamResult IS NOT NULL
        begin
           insert into @CustomerIdList (id) VALUES (@ParamResult)

            set @RecordCount = @RecordCount + 1
            set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
        end
    end     
    
    declare @VatCodeId int
    select @VatCodeId = VatCodeID from StandardVatRate
    select @VatRate = 17.5
    select @VatRate = PercentageValue from VatCode
		where ID = @VatCodeId					
	        		
	select c.ID, 
			name, 
			p.ProductCode, 
			p.Description,
		case when p.VatExempt = 1 then sum(round((ol.Price * ol.NoOfUnits) - isnull(ocnl.Price, 0),2))
			else sum( round((ol.Price * ol.NoOfUnits) * (1 + (@VatRate/ 100.0)),2) - round(isnull(ocnl.Price, 0) * (1 + (@VatRate/ 100.0)),2))  end Price			
		from OrderHeader oh
		inner join OrderLine ol on
			oh.ID = ol.OrderID
		inner join Customer c on 
			oh.CustomerID = c.ID
		inner join Product p on
			ol.ProductID = p.ID							
		inner join VatCode v on 
			oh.VatCodeID = v.ID	
		left join OrderCreditNoteLine ocnl on
			ol.ID = ocnl.OrderLineID				
		where c.ID in (select id from @CustomerIdList) and
			oh.OrderStatus in (2,3,6,7)						
		    and oh.DeliveryDate between @p_DateFrom and @p_DateTo
		group by c.ID, 
			name,
			p.ProductCode, 
			p.Description,
			p.VatExempt
		order by Price desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@p_CustomerIdList">
            <Value>=Join(Parameters!p_CustomerIdList.Value,":")</Value>
          </QueryParameter>
          <QueryParameter Name="@p_DateFrom">
            <Value>=Parameters!p_DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_DateTo">
            <Value>=Parameters!p_DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
