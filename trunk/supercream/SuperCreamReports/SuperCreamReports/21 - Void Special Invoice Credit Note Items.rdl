      <Query>
        <DataSourceName>SCDataSource</DataSourceName>
        <CommandText>SELECT        SpecialInvoiceHeader.InvoiceNo, SpecialInvoiceCreditNote.CreditAmount, SpecialInvoiceCreditNote.Reason, SpecialInvoiceCreditNote.DateCreated, 
                         SpecialInvoiceCreditNote.Reference, SpecialInvoiceCreditNote.VatExempt, SpecialInvoiceCreditNote.ReasonForVoiding
FROM            SpecialInvoiceCreditNote INNER JOIN
                         SpecialInvoiceHeader ON SpecialInvoiceCreditNote.SpecialInvoiceID = SpecialInvoiceHeader.ID
WHERE        (SpecialInvoiceCreditNote.DueDate BETWEEN @DateStart AND @DateEnd) AND (SpecialInvoiceCreditNote.IsVoid = 1)
ORDER BY SpecialInvoiceCreditNote.DueDate DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DateStart">
            <Value>=Parameters!DateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateEnd">
            <Value>=Parameters!DateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
