          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RRPPerItem">
          <DataField>RRPPerItem</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="UnitQty">
          <DataField>UnitQty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NetPrice">
          <DataField>NetPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        oh.ID AS OrderID, vn.ID AS VanID, vn.Description, oh.DeliveryDate, oh.AlphaID, oh.InvoiceNo, p.ProductCode, p.Description AS ProductDescription, ol.NoOfUnits, 
                         p.RRPPerItem, p.UnitQty, ol.Price * ol.NoOfUnits AS NetPrice, 
                         CASE WHEN vc.PercentageValue &gt; 0 THEN round(ol.NoOfUnits * ol.Price * (1 + (vc.PercentageValue / 100)), 2) ELSE round(ol.NoOfUnits * ol.Price, 2) 
                         END AS Price
FROM            OrderHeader AS oh INNER JOIN
                         OrderNotesStatus AS os ON oh.ID = os.OrderID INNER JOIN
                         Van AS vn ON os.VanID = vn.ID INNER JOIN
                         OrderLine AS ol ON oh.ID = ol.OrderID INNER JOIN
                         Product AS p ON ol.ProductID = p.ID INNER JOIN
                         VatCode AS vc ON oh.VatCodeID = vc.ID
WHERE        (oh.DeliveryDate = @StartDate) AND (oh.OrderStatus = dbo.ConstInvoice() OR
                         oh.OrderStatus = dbo.ConstInvoicePrinted() OR
                         oh.OrderStatus = dbo.ConstDeliveryNote() OR
                         oh.OrderStatus = dbo.ConstDeliveryNotePrinted())
ORDER BY oh.DeliveryDate DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
