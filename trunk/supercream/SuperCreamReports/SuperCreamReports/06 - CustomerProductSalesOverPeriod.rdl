        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>CustomerProductsDS</DataSourceName>
        <CommandText>declare @RecordCount int,
		@ParamResult varchar(50)		
		
declare @VatRate decimal(10,2)					
declare @CustomerIdList table (id int)
		
 
    if @p_CustomerIdList is not null

    begin
        set @RecordCount = 1
        set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
 
        while @ParamResult IS NOT NULL
        begin
           insert into @CustomerIdList (id) VALUES (@ParamResult)

            set @RecordCount = @RecordCount + 1
            set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_CustomerIdList, @RecordCount)
        end
    end     
    
    declare @ProductIdList table (id int)		
 
    if @p_ProductList is not null

    begin
        set @RecordCount = 1
        set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_ProductList , @RecordCount)
 
        while @ParamResult IS NOT NULL
        begin
           insert into @ProductIdList (id) VALUES (@ParamResult)

            set @RecordCount = @RecordCount + 1
            set @ParamResult = dbo.RPT_GetColonDelimitedParamItem(@p_ProductList , @RecordCount)
        end
    end     

 declare @VatCodeId int
    select @VatCodeId = VatCodeID from StandardVatRate
    select @VatRate = PercentageValue from VatCode
		where ID = @VatCodeId					
	        		
	select c.ID, 
			name, 
			p.ProductCode, 
			p.Description,						
			datepart(mm, oh.DeliveryDate) Month,
			datepart(year, oh.DeliveryDate) Year,					
		case when p.VatExempt = 1 then round(sum((ol.Price * ol.NoOfUnits) - isnull(ocnl.Price, 0)),2)
			else Round(sum(ol.Price * ol.NoOfUnits * (1 + (@VatRate/ 100.0))) - sum(isnull(ocnl.Price, 0) * (1 + (@VatRate/ 100.0))),2) end Price
		from OrderHeader oh
		inner join OrderLine ol on
			oh.ID = ol.OrderID
		inner join Customer c on 
			oh.CustomerID = c.ID
		inner join Product p on
			ol.ProductID = p.ID	
		left join OrderCreditNoteLine ocnl on
			ol.ID = ocnl.OrderLineID						
		where c.ID in (select id from @CustomerIdList) and
			p.ID in (select id from @ProductIdList) and
			oh.OrderStatus in (2,3,6,7)			
			and oh.DeliveryDate between @p_DateFrom and @p_DateTo
		group by c.ID, 
			name,
			p.ProductCode, 
			p.Description,
			p.VatExempt,
			datepart(mm, oh.DeliveryDate),
			datepart(year, oh.DeliveryDate)			
		order by Month, Price desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@p_DateFrom">
            <Value>=Parameters!p_DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_DateTo">
            <Value>=Parameters!p_DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@p_CustomerIdList">
            <Value>=Join(Parameters!p_CustomerIdList.Value,":")</Value>
          </QueryParameter>
          <QueryParameter Name="@p_ProductList">
            <Value>=Join(Parameters!p_ProductList.Value,":")</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
