  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SCDataSource</DataSourceName>
        <CommandText>SELECT        TOP (10) c.Name, SUM(CASE WHEN p.VatExempt = 1 THEN round((ol.Price * ol.NoOfUnits) - isnull(ocnl.Price, 0), 2) 
                         ELSE round((ol.Price * ol.NoOfUnits * (1 + (v.PercentageValue / 100.0))) - isnull(ocnl.Price, 0) * (1 + (v.PercentageValue / 100.0)), 2) END) 
                         - ISNULL(ROUND(SUM(CASE WHEN cr.VatExempt = 0 THEN cr.CreditAmount ELSE cr.CreditAmount * (1 + (v.PercentageValue / 100.0)) END), 2), 0) AS Price
FROM            OrderHeader AS oh INNER JOIN
                         Customer AS c ON oh.CustomerID = c.ID INNER JOIN
                         OrderLine AS ol ON oh.ID = ol.OrderID INNER JOIN
                         Product AS p ON ol.ProductID = p.ID INNER JOIN
                         VatCode AS v ON oh.VatCodeID = v.ID LEFT OUTER JOIN
                         OrderCreditNoteLine AS ocnl ON ol.ID = ocnl.OrderLineID LEFT OUTER JOIN
                         CreditNote AS cr ON oh.ID = cr.OrderID
WHERE        (oh.OrderStatus IN (2, 3, 6, 7)) AND (oh.DeliveryDate &gt;= @StartDate) AND (oh.DeliveryDate &lt;= @EndDate)
GROUP BY c.Name
ORDER BY Price DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SCDataSource</DataSourceName>
        <CommandText>SELECT SUM(NoOfUnits * Price) AS Price FROM OrderHeader oh
INNER JOIN OrderLine ol ON
	oh.ID = ol.OrderID
INNER JOIN Product p ON
	p.ID = ol.ProductID
WHERE OrderStatus IN (2,3,6,7) AND p.VatExempt = 1
 AND (oh.DeliveryDate &gt;= @StartDate) AND (oh.DeliveryDate &lt;= @EndDate)
ORDER BY Price DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
