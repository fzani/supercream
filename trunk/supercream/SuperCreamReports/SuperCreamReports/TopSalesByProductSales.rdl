    <DataSource Name="SCDataSource">
      <DataSourceReference>SCDataSource</DataSourceReference>
      <rd:DataSourceID>44cb7793-a8bc-49a7-aad8-1fcbe0bd1a4f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ProductCode">
          <DataField>ProductCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ExVatPrice">
          <DataField>ExVatPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PriceInclVat">
          <DataField>PriceInclVat</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SCDataSource</DataSourceName>
        <CommandText>WITH cte(ProductCode, Description, ExVatPrice, PriceInclVat) AS 
(
	select top 1000 
		p.ProductCode,
		p.Description
		,round(sum(NoOfUnits * Price), 2)
		,case when p.VatExempt = 1 then round(sum(NoOfUnits * Price), 2) else round(sum(NoOfUnits * Price * (1 + (v.PercentageValue / 100))), 2) end	as ExVatPrice
  FROM [SuperCreamDB].[dbo].[OrderHeader] oh
  inner join OrderLine ol on
	oh.ID = ol.OrderID
  inner join Product p on p.ID = ol.ProductID
  inner join VatCode v on oh.VatCodeID = v.ID
  where oh.OrderStatus IN (2,3,6,7)  
	and  oh.DeliveryDate &gt;= @DateStarted AND oh.DeliveryDate &lt;= @DateEnded	
  group BY p.ProductCode, p.Description, p.VatExempt	   
   )
   SELECT * FROM cte  
	where ((ISNULL(@PriceValue, '-1')) = '-1' OR PriceInclVat &gt;= @PriceValue)
   ORDER BY ExVatPrice DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@PriceValue">
            <Value>=Parameters!PriceValue.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateStarted">
            <Value>=Parameters!DateStarted.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateEnded">
            <Value>=Parameters!DateEnded.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
