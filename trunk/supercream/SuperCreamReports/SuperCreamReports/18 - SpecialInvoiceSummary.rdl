      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	DECLARE @Total DECIMAL(10,2),
			@VatTotal DECIMAL(10,2),
			@NetTotal DECIMAL(10,2),
			@NonVatableTotal DECIMAL(10,2),
			@VatableTotal DECIMAL(10,2),
			@PercentageValue varchar(10),
			@CreditTotal decimal(10,2),
			@CreditVatTotal decimal(10,2),
			@CreditNetTotal decimal(10,2),
			@CreditNonVatableTotal decimal(10,2),
			@CreditVatableTotal decimal(10,2)			
					
	SET @Total = 0
	SET @VatTotal = 0
	SET @NetTotal = 0
	SET @NonVatableTotal = 0
	SET @VatableTotal = 0
	SET @PercentageValue = 0;
		
	with cte(NetTotal)
	as
	(
		select case when sil.Discount = 0 then isnull(sil.NoOfUnits * sil.Price, 0)
			else isnull((sil.NoOfUnits * sil.Price) - ((sil.NoOfUnits * sil.Price) * sil.Discount / 100.0),0)
			end as id from SpecialInvoiceHeader sih
		inner join SpecialInvoiceLine sil ON
			sih.ID = sil.SpecialInvoiceID				
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)	
	select @NetTotal = sum(NetTotal) from cte;					
					
	with VatableTotal(total)
	as
	(
		select case when sil.Discount = 0 then 
				case when sil.VatExempt = 1 then
					0
			else
			  		Round(isnull(sil.NoOfUnits * sil.Price, 0) *  (1 + (vat.PercentageValue / 100.0)),2)
			end
			else 
				case when sil.VatExempt = 1 then			
					0
					
				else 
					isnull(Round(((sil.NoOfUnits * sil.Price) - ((sil.NoOfUnits * sil.Price) * sil.Discount / 100.0))
						* (1 + (vat.PercentageValue / 100.0)),2),0)	
			end
			end as id from SpecialInvoiceHeader sih
		inner join SpecialInvoiceLine sil ON
			sih.ID = sil.SpecialInvoiceID					
		inner join VatCode vat on
			sih.VatCodeID = vat.ID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate			
	)
	select @VatableTotal = sum(total) from VatableTotal;
	
	with VatTotal(total)
	as
	(
		select case when sil.Discount = 0 then 
				case when sil.VatExempt = 1 then
					0
			else
			  	Round(isnull(sil.NoOfUnits * sil.Price, 0) * (vat.PercentageValue / 100.0),2)
			end
			else 
				case when sil.VatExempt = 1 then			
					0
				else 				
					isnull(Round(((sil.NoOfUnits * sil.Price) - ((sil.NoOfUnits * sil.Price) * sil.Discount / 100.0))
						* ((vat.PercentageValue / 100.0)),2),0)					
			end
			end as id from SpecialInvoiceHeader sih
		inner join SpecialInvoiceLine sil ON
			sih.ID = sil.SpecialInvoiceID					
		inner join VatCode vat on
			sih.VatCodeID = vat.ID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate					
	)
	select @VatTotal = sum(total) from VatTotal;
	
	
	with NonVatableTotal(total)
	as
	(
		select case when sil.Discount = 0 then 
				case when sil.VatExempt = 1 then
					isnull(sil.NoOfUnits * sil.Price, 0)
			else
			  		0
			end
			else 
				case when sil.VatExempt = 1 then			
					isnull((sil.NoOfUnits * sil.Price) - ((sil.NoOfUnits * sil.Price) * sil.Discount / 100.0),0)
					
				else 
					0
			end
			end as id from SpecialInvoiceHeader sih
		inner join SpecialInvoiceLine sil ON
			sih.ID = sil.SpecialInvoiceID					
		inner join VatCode vat on
			sih.VatCodeID = vat.ID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @NonVatableTotal = total from NonVatableTotal;
	
	with CreditTotal(Total) as
	(
		select case when sic.VatExempt = 1 then CreditAmount else Round(CreditAmount * (1 + (v.PercentageValue / 100)),2)  end TotalCredit from SpecialInvoiceHeader sih
		inner join VatCode v 
			on sih.VatCodeID = v.Id
		left join SpecialInvoiceCreditNote sic
			on sih.ID = sic.SpecialInvoiceID		
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @CreditTotal = sum(Total) from CreditTotal;
			
	with CreditVatTotal(Total) as
	(
		select case when sic.VatExempt = 1 then 0 else Round(CreditAmount * (v.PercentageValue / 100),2)  end TotalCredit from SpecialInvoiceHeader sih
			inner join VatCode v 
			on sih.VatCodeID = v.Id
		left join SpecialInvoiceCreditNote sic
			on sih.ID = sic.SpecialInvoiceID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @CreditVatTotal = sum(Total) from CreditVatTotal;
		
	with CreditNetTotal(Total) as
	(
		select round(CreditAmount, 2) TotalCredit from SpecialInvoiceHeader sih
			inner join VatCode v 
			on sih.VatCodeID = v.Id
		left join SpecialInvoiceCreditNote sic
			on sih.ID = sic.SpecialInvoiceID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @CreditNetTotal = sum(Total) from CreditNetTotal;
	
	with CreditNonVatableTotal(Total) as
	(
		select case when sic.VatExempt = 1 then round(CreditAmount,2) else 0 end TotalCredit from SpecialInvoiceHeader sih
			inner join VatCode v 
			on sih.VatCodeID = v.Id
		left join SpecialInvoiceCreditNote sic
			on sih.ID = sic.SpecialInvoiceID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @CreditNonVatableTotal = sum(Total) from CreditNonVatableTotal;
	
	
	with CreditVatableTotal(Total) as
	(
		select case when sic.VatExempt = 1 then 0 else round(CreditAmount * (1 + (v.PercentageValue / 100.0)),2)    end TotalCredit from SpecialInvoiceHeader sih
			inner join VatCode v 
			on sih.VatCodeID = v.Id
		left join SpecialInvoiceCreditNote sic
			on sih.ID = sic.SpecialInvoiceID
		where sih.DeliveryDate between @DeliveryStartDate and @DeliveryEndDate		
	)
	select @CreditVatableTotal = sum(Total) from CreditVatableTotal;
												
	SET @Total = @NetTotal + @VatTotal
	
	SELECT @PercentageValue = ISNULL(PercentageValue, 0) FROM VatCode v
		INNER JOIN SpecialInvoiceHeader sih ON v.ID = sih.VatCodeID		
		
	SELECT @Total AS Total, @VatTotal AS VatTotal, @NetTotal AS NetTotal, @NonVatableTotal AS NonVatableTotal, @VatableTotal AS VatableTotal, @PercentageValue AS PercentageValue,
			@CreditTotal CreditTotal, @CreditVatTotal CreditVatTotal, @CreditNetTotal CreditNetTotal, @CreditNonVatableTotal CreditNonVatableTotal,
			@CreditVatableTotal CreditVatableTotal

</CommandText>
        <QueryParameters>
